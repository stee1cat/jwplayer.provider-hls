{
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "**/typings/**"
    ]
  },
  "rules": {
    "no-var-requires": false,
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "only-arrow-functions": false,
    "trailing-comma": [
      false,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "interface-name": [
      true,
      "always-prefix"
    ],
    "no-namespace": false,
    "no-bitwise": false,
    "no-console": [
      true,
      "time",
      "timeEnd",
      "trace"
    ],
    "max-line-length": [
      false,
      100
    ],
    "space-before-function-paren": [
      true,
      {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "always",
        "method": "never"
      }
    ],
    "prefer-const": false,
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-module",
      "check-operator",
      "check-separator",
      "check-type"
    ],
    "import-destructuring-spacing": true,
    "no-string-literal": false,
    "no-use-before-declare": true,
    "object-literal-sort-keys": false,
    "ordered-imports": true,
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "variable-name": [
      true,
      "allow-pascal-case",
      "ban-keywords",
      "check-format"
    ]
  }
}
